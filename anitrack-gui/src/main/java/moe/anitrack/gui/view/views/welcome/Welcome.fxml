<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="moe.anitrack.gui.view.views.welcome.WelcomeController">
    <top>
        <HBox alignment="CENTER" spacing="5.0" BorderPane.alignment="CENTER">
            <children>
                <Label alignment="BASELINE_CENTER" maxHeight="1.7976931348623157E308" text="Synchronizing with:"/>
                <Label fx:id="providerName" alignment="BASELINE_CENTER" maxHeight="1.7976931348623157E308" text="CURRENT_PROVIDER">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
    </top>
    <center>
        <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <ImageView fx:id="providerLogo" fitHeight="128.0" fitWidth="128.0" pickOnBounds="true" preserveRatio="true"/>
            </children>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
        </HBox>
    </center>
</BorderPane>
